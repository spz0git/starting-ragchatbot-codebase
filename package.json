{
  "name": "course-materials-assistant-frontend",
  "version": "0.1.0",
  "description": "Frontend for Course Materials RAG Chatbot",
  "type": "module",
  "scripts": {
    "format": "./format-frontend.sh",
    "lint": "eslint frontend/ --format=compact",
    "lint:fix": "eslint frontend/ --fix --format=compact",
    "check-quality": "./check-frontend-quality.sh",
    "format:check": "prettier --check frontend/",
    "format:write": "prettier --write frontend/",
    "quality": "npm run lint && npm run format:check"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0"
  },
  "keywords": [
    "rag",
    "chatbot",
    "course-materials",
    "ai"
  ],
  "author": "",
  "license": "MIT"
}
